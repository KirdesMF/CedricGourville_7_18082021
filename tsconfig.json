{
   "compilerOptions": {
      "target": "es2020",
      "lib": ["esnext"],
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "incremental": true,
      "skipLibCheck": true,
      "importHelpers": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "moduleResolution": "node",
      "module": "commonjs"
   },
   "exclude": ["node_modules", "dist", ".cache"]
}
